; Example case: Channel3D

RunControl
{
	terminationCondition iterations
	maxNumberOfIterations 1000
	fileWriteInterval 1
	timeStep 2e-3
	maxSimTime 48
	maxRealTimeHours 48

}

Solver
{
	timeAccurate OFF
	relaxationFactorMomentum 0.7
	relaxationFactorPCorr 0.3
	rho 1
	mu 0.001
	numberOfPressureCorrections 3
	numberOfInnerIterations 4
}

Boundaries
{
	east
	{
		type outlet
		refVector (0,0,0)
		refValue 0
	}
	west
	{
		type inlet
		refVector (1,0,0)
		refValue 0
	}
	north
	{
		type wall
		refVector (0,0,0)
		refValue 0
	}
	south
	{
		type wall
		refVector (0,0,0)
		refValue 0
	}
	top
	{
		type wall
		refVector (0,0,0)
		refValue 0
	}
	bottom
	{
		type wall
		refVector (0,0,0)
		refValue 0
	}
}
