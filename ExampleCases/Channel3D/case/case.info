; Example case: Channel3D

RunControl
{
	terminationCondition iterations
	maxNumberOfIterations 1000
	fileWriteInterval 1
	timeStep 2e-3
	maxSimTime 48
	maxRealTimeHours 48

}

Solver
{
	timeAccurate ON
	relaxationFactorMomentum 0.5
	relaxationFactorPCorr 0.5
	rho 998
	mu 8.94e-4
	numberOfPressureCorrections 3
	numberOfInnerIterations 4
}

Boundaries
{
	east
	{
		type outlet
		refVector (0,0,0)
	}
	west
	{
		type inlet
		refVector (1,0,0)
	}
	north
	{
		type wall
		refVector (0,0,0)
	}
	south
	{
		type wall
		refVector (0,0,0)
	}
	top
	{
		type wall
		refVector (0,0,0)
	}
	bottom
	{
		type wall
		refVector (0,0,0)
	}
}
